<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.oceanbrasil.tdcexample">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <meta-data
            android:name="com.google.android.gms.version"
            android:value="@integer/google_play_services_version" />

        <meta-data
            android:name="com.google.android.geo.API_KEY"
            android:value="@string/key_maps"/>

        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <!-- activities de examplos INICIO-->
            <activity
                android:name=".activities.ActivityAgrupamento"
                android:parentActivityName=".MainActivity"/>
            <activity
                android:name=".activities.ActivityGeoFire"
                android:parentActivityName=".MainActivity"/>
            <activity android:name=".activities.ActivityEstiloMap"
                android:parentActivityName=".MainActivity"/>
        <!-- activities de examplos FIM-->

    </application>

</manifest>